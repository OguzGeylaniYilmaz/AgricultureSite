@model List<Address>
<h3 class="tittle">Contact Us</h3>
<p class="heading-btm-w3ls">Address Info</p>
@foreach (var item in Model)
{
    <div class="col-md-3 contact-left">
        <h4>Our Address</h4>
        <p>@item.Description1</p>
        <p>@item.Description3</p>
        <h4>Phone</h4>
        <p>@item.Description2</p>
    </div>
}
